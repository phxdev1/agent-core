# OpenRouter API Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here

# LLM Configuration
LLM_MODEL=mistralai/mistral-medium-3.1
LLM_MAX_TOKENS=2048
LLM_TEMPERATURE=0.7

# Redis Configuration (optional - will use in-memory if not set)
# For Redis Cloud, use the connection details from your Redis Cloud dashboard
REDIS_HOST=redis-11364.c24.us-east-mz-1.ec2.redns.redis-cloud.com
REDIS_PORT=11364
REDIS_USERNAME=default
REDIS_PASSWORD=your_redis_password_here
REDIS_DB=0

# Research APIs (optional)
SERPAPI_KEY=your_serpapi_key_here
ARXIV_MAX_RESULTS=5
SCHOLAR_MAX_RESULTS=5

# System Configuration
MEMORY_WINDOW_SIZE=20
CONTEXT_WINDOW_SIZE=4
ENABLE_GPU=false
LOG_LEVEL=INFO
